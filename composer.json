{
  "name": "reach-digital/spotler-client",
  "description": "PHP Client for Spotler (spotler.nl) with support for Laravel 5.5 and higher",
  "type": "library",
  "keywords": [
    "spotler",
    "mailplus"
  ],
  "require-dev": {
    "php": ">=7.1.3",
    "orchestra/testbench": "^3.5",
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.0",
    "squizlabs/php_codesniffer": "^3.1",
    "psy/psysh": "^0.8.13"
  },
  "require": {
    "ext-curl": "*",
    "ext-json": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "St√®phan Eizinga",
      "email": "stephan@monkeysoft.nl"
    }
  ],
  "autoload": {
    "psr-4": {
      "Spotler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Spotler\\SpotlerServiceProvider"
      ],
      "aliases": {
        "Spotler": "Spotler\\SpotlerFacade"
      }
    }
  }
}
